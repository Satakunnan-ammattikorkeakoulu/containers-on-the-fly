meta {
  name: Get Single Container
  type: http
  seq: 2
}

get {
  url: {{root_url}}/admin/container?containerId=1
  body: none
  auth: bearer
}

auth:bearer {
  token: {{admin_token}}
}

tests {
  test("Status code is 200", function() {
    expect(res.status).to.equal(200);
  });
  
  test("Response is successful", function() {
    expect(res.body).to.have.property('status');
    expect(res.body.status).to.equal('success');
  });
  
  test("Response has container data", function() {
    expect(res.body).to.have.property('data');
    expect(res.body.data).to.have.property('data');
    const container = res.body.data.data;
    expect(container).to.have.property('name');
    expect(container).to.have.property('imageName');
    expect(container).to.have.property('description');
  });
}