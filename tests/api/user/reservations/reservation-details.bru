meta {
  name: Get Own Reservation Details
  type: http
  seq: 8
}

get {
  url: {{root_url}}/reservation/get_own_reservation_details
  body: none
  auth: bearer
}

auth:bearer {
  token: {{user_token}}
}

params:query {
  reservationId: 1
}

tests {
  test("Status code is 200", function() {
    expect(res.status).to.equal(200);
  });
  
  test("Response is successful", function() {
    expect(res.body).to.have.property('status');
    expect(res.body.status).to.equal('success');
  });
  
  test("Response has reservation details", function() {
    expect(res.body).to.have.property('data');
    expect(res.body.data).to.have.property('reservation');
    expect(res.body.data.reservation).to.have.property('id');
    expect(res.body.data.reservation).to.have.property('startTime');
    expect(res.body.data.reservation).to.have.property('endTime');
  });
}